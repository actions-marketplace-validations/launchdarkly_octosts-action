{
	"name": "octosts-action",
	"version": "1.1.0",
	"description": "Generate a short-lived GitHub Token using OctoSTS",
	"type": "module",
	"exports": "dist/run/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/launchdarkly-labs/octosts-action.git"
	},
	"scripts": {
		"build": "tsc --outDir $(mktemp -d)",
		"release": "concurrently \"ncc build src/run/index.ts --target es2020 -o dist/run\" \"ncc build src/post-run/index.ts --target es2020 -o dist/post-run\"",
		"check-all": "concurrently \"npm:format-check\" \"npm:lint\" \"npm:build\"",
		"format": "biome format --write",
		"format-check": "biome check src/**/*.ts",
		"lint": "biome lint src/**/*.ts"
	},
	"keywords": [
		"Actions",
		"GitHub",
		"ChainGuard",
		"octo-sts"
	],
	"author": "The LaunchDarkly Team",
	"license": "Apache-2.0",
	"packageManager": "pnpm@10.18.1+sha512.77a884a165cbba2d8d1c19e3b4880eee6d2fcabd0d879121e282196b80042351d5eb3ca0935fa599da1dc51265cc68816ad2bddd2a2de5ea9fdf92adbec7cd34",
	"dependencies": {
		"@actions/core": "^3.0.0",
		"@actions/exec": "^3.0.0",
		"@actions/github": "^9.0.0",
		"undici": "^7.20.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"@types/node": "^25.2.0",
		"@vercel/ncc": "^0.38.4",
		"concurrently": "^9.2.1",
		"typescript": "^5.9.3"
	}
}
